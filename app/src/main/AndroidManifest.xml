<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".dummy"
            android:exported="false" />
        <activity
            android:name=".SplashscreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IccRankingActivity"
            android:exported="false" />
        <activity
            android:name=".LiveScoreActivity"
            android:exported="false" />
        <activity
            android:name=".LiveMatch_activity"
            android:exported="false" />
        <activity
            android:name=".Match_Detail"
            android:exported="false" />
        <activity
            android:name=".season_Activity"
            android:exported="false" />
        <activity
            android:name=".match_Activity"
            android:exported="false" />
        <activity
            android:name=".compition_Activity"
            android:exported="false" />
        <activity
            android:name=".Main_Activity"
            android:exported="true"></activity>
    </application>

</manifest>